{
    "compilerOptions": {
      "outDir": "./dist/",
      "target": "es2020",
      "module": "NodeNext",
      "moduleResolution": "nodenext",
      "strict": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"],
  }